      "$description": "Font family definitions for demo company typography",
      "brand": {
        "primary": {
          "value": [
            "-apple-system",
            "BlinkMacSystemFont",
            "Segoe UI",
            "Roboto",
            "Oxygen",
            "Ubuntu",
            "Cantarell",
            "sans-serif"
          ],
          "type": "fontFamily",
          "$description": "Primary brand font for UI and most content"
        }
      }
